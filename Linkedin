/*
Question: Duplicate Job Listings
by: Linkedin
Link to the detail of question: https://datalemur.com/questions/duplicate-job-listings
*/

WITH duplicate_jobs
AS (
  SELECT
    company_id,
    title,
    description,
    COUNT(*)
  FROM
    job_listings
  GROUP BY
    company_id, title, description
  HAVING COUNT(*) > 1)

SELECT
  COUNT(company_id) AS duplicate_companies
FROM
  duplicate_jobs;
