/*
Question: 
by: Adobe
Link to the detail of question: 
Approach: LEFT JOIN
*/

SELECT
  p.page_id
FROM
  pages p
  LEFT JOIN
  page_likes l ON p.page_id = l.page_id
WHERE
  l.user_id IS NULL
ORDER BY p.page_id;

--using NOT EXISTS

SELECT
  page_id
FROM
  pages
WHERE NOT EXISTS
  (SELECT
    page_id
    FROM
    page_likes AS likes
    WHERE
    pages.page_id = likes.page_id);
